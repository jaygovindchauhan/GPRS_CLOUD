@model GPRS_CLOUD.Models.HistoricalDataM
@{
    ViewBag.Title = "Index";
}

<style>
    th{
     
        font-weight:normal;

    }

</style>

@*<p>Date: <input type="text" id="datepicker"></p>*@


<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript">    </script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)
    <fieldset>
        

        <style>
            .mt50 {
                margin-top: 50px;
            }

            .mb20 {
                margin-bottom: 20px;
            }

            .mt20 {
                margin-top: 20px;
            }

            .nav-tabs {
                background-color: #f1f1f1;
            }

            .active {
                border-top: 2px solid #337ab7;
            }

            .custombtn {
                width: 30%;
            }

            .font18 {
                font-size: 18px;
            }

            .well {
                padding: 10px !important;
            }
         
        </style>



        <ul class="nav nav-tabs nav-justified mt50">
            <li><a href="/Live/Index">Home</a></li>
            <li><a href="#">Dashboard</a></li>
            <li><a href="/Graph/Index">Graph</a></li>
            <li class="active"><a href="#">Historical Data</a></li>
            <li><a href="#">Environment Benefit</a></li>
            <li><a href="#">Site Information</a></li>
        </ul>


        <div class="col-md-12 well mt20 font16">Historical Data</div>
        <div style="clear:both;"></div>


        <div class="row">
            <div class="editor-label col-md-3">
                @Html.LabelFor(model => model.StartDate)
           
           
                @Html.EditorFor(model => model.StartDate)
                @Html.ValidationMessageFor(model => model.StartDate)
            </div>
            
            <div class="editor-label col-md-3">
                @Html.LabelFor(model => model.EndDate)
           
                @*@Html.EditorFor(model => model.News_Date)*@
                @Html.EditorFor(model => model.EndDate@*, new { @class = "form-control datepicker" }*@)
                @Html.ValidationMessageFor(model => model.EndDate)
            </div>

            <p>
                <span><input type="submit" value="Show" class="btn btn-primary" /> </span> <span ><a class="col-md-2">Export Data</a></span>
            </p>
        </div>
        
    </fieldset>
}

<table class="table table-striped table-bordered text-center " style="padding:4px 15px 2px 4px; margin-top: 23px;" >
    <tr>
        <th>
            <span>Date</span>
        </th>
        <th>
            <span>Time</span>
        </th>
        <th>
            <span>DC CURRENT</span>
        </th>
        <th>
            <span>Toatal Energy Generation</span>
        </th>
        <th>
            <span>Motor AMP</span>
        </th>
        <th>
            <span>Motor Voltage</span>
        </th>
        <th>
            <span>Motor Speed</span>
        </th>
        <th>
            <span>Motor Kw</span>
        </th>
    </tr>
    </table>



@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)
    <fieldset>



